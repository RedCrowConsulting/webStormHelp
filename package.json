{
  "name": "webStormHelp",
  "version": "0.2.0",
  "description": "a private starter kit.",
  "scripts": {
    "//": [
      "npm start -s FOR development,",
      "npm run build -s FOR dist folder refresh & to serve production locally",
      "npm run deploy -s FOR surge",
      "npm run share -s for a localtunnel session",
      "for localtunnel avoid --subdomain name (as they fail more often)"
    ],
    "prestart": "npm-run-all --parallel open:prestart",
    "open:prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run test -- --watch",
    "lint": "esw webpack.config.* src buildScripts --color",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "prebuild": "npm-run-all open:distMessage clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "npm-run-all --parallel start-mockapi open:dist",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "open:distMessage": "babel-node buildScripts/buildDistMessage.js",
    "open:dist": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist",
    "preshare": "babel-node buildScripts/shareMessage.js",
    "share": "npm-run-all --parallel open:src localtunnel",
    "localtunnel": "lt --port 3000",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "generate-mock-data": "babel-node buildScripts/mockDataGenerator.js",
    "version": "node -p \"require('./package.json').version\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedCrowConsulting/webStormHelp.git"
  },
  "keywords": [],
  "author": "rudimusmaximus <rudy@redcrowconsulting.com> (https://redcrowconsulting.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RedCrowConsulting/webStormHelp/issues"
  },
  "homepage": "https://github.com/RedCrowConsulting/webStormHelp#readme",
  "dependencies": {
    "@material/button": "^4.0.0",
    "@material/icon-button": "^4.0.0",
    "@material/ripple": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.2.1",
    "json-schema-faker": "^0.5.0-rc21",
    "json-server": "^0.15.1",
    "localtunnel": "^2.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^10.2.0",
    "nock": "^11.7.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.3",
    "numeral": "^2.0.6",
    "open": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "shortid": "^2.2.15",
    "style-loader": "^1.0.0",
    "surge": "^0.21.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-md5-hash": "0.0.6"
  }
}
